QC_OUTDIR=fastqc_out
QC_RAW_OUTDIR=$(addsuffix /raw, $(QC_OUTDIR))

qc_out:
	mkdir $(QC_OUTDIR)

qc_raw_out: qc_out
	mkdir $(QC_RAW_OUTDIR)

qc_raw: $(RAWDATA) qc_raw_out
	fastqc -o $(QC_RAW_OUTDIR) -t $(words $(RAWDATA))  $(RAWDATA)

qc_clean: $(QC_OUTDIR)
	rm -r $(QC_OUTDIR)

